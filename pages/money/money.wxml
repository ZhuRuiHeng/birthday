<!--pages/money/money.wxml-->
<view class="container">
	<view class="balanceTitle">
		余额
	</view>
	<view class="balanceNum">
		<view class="currency">
			￥</view>{{money}}
		<view class="currency">
		</view>
	</view>
	<view class="addOrSub">
		<navigator class="action-group" url="../chong/chong">
			<image class="action-icon" src="../images/pay.png"></image>
			<view class="action-label">充值</view>
		</navigator>
		<navigator class="action-group" url="../tixian/tixian">
			<image class="action-icon" src="../images/tixian.png"></image>
			<view class="action-label">提现</view>
		</navigator>
	</view>
	<view class="nextPageView">
		<navigator url="../keep/keep" class="moneyDetail">流水明细</navigator>
		<navigator class="explain" url="../notice/notice"> 钱包说明</navigator>
		<image class="headerIcon" src="../images/que.png"></image>
	</view>
</view>
