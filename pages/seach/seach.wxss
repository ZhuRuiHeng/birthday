.continer{
     width: 100%;
     height: 100%;
     background-color: #fff;
     padding-top: 1px;
}
 .header-nav {
     background: #fff;
     -webkit-align-items: center;
     align-items: center;
     padding: 9px 19px 0 19px;
}
 .search-container {
     position: relative;
     height: 32px;
     color: #FFF;
     border-radius:3px;
     z-index: 100;
     overflow: hidden;
}
 .search-active {
     margin-right: 52px;
}
 .search {
     position: absolute;
     z-index: 110;
     right: 16px;
     top: 12px;
     font-size: 15px;
     color: #333333;
     line-height: 29px;
     padding: 0 9px;
}
 .search-container wx-input {
     background: #F9F9F9;
     color: #333333;
     padding: 0 28px;
     font-size: 13px;
     height: 100%;
     border-radius: 3px;
     padding-right: 14px;
}
 .search-container wx-icon {
     position: absolute;
     z-index: 10;
     left: 8px;
     top: 8px;
}
 .search-container .closeIco{
     position: absolute;
     z-index: 10;
     top: 9px;
     right: 7px;
     width: 14px;
     height: 14px;
     border-radius: 50%;
     line-height: 14px;
     text-align: center;
     font-size: 13px;
     background: rgb(194,194,194);
     color: #fff;
}
 .content{
     margin: 9px auto 0 auto;
     width: 100% ;
     background: #F9F9F9;
}
 .mayFriends{
     padding: 1px;
     width: 100%;
     height: 321px;
     box-sizing: border-box;
}
 .mayFriends .title{
     margin: 19px auto;
     width: 100%;
     text-align: center;
     font-family: PingFangSC-Regular;
     font-size: 13px;
     color: #999999;
}
 .mayFriends .friendsInfo{
     margin: 0 auto;
     width: 331px;
     height: 240px;
     float: right;
     white-space: nowrap;
     background-color: #F9F9F9;
}
 .mayFriends .friendsInfo .item{
     display: inline-block;
     margin-right: 4px;
     width: 148px;
     height: 100%;
     background-color: #ffffff;
     box-sizing: border-box;
}
 .mayFriends .friendsInfo .item .more{
     position: absolute;
     top: 0;
     left: 0;
     width: 148px;
     height: 100%;
     background: url('http://ovhvevt35.bkt.clouddn.com/day/srgjmore@2x.jpg');
     background-repeat: no-repeat;
     background-size: 100%;
}
 .mayFriends .friendsInfo .item wx-image{
     margin: 19px auto 17px auto;
     width: 57px;
     height: 57px;
     border-radius: 50%;
}
 .mayFriends .friendsInfo .item .name{
     width: 100%;
     height: 16px;
     line-height: 16px;
     text-align: center;
     font-family: PingFangSC-Regular;
     font-size: 15px;
     color: #333333;
     overflow: hidden;
}
 .mayFriends .friendsInfo .item .date{
     margin: 6px 0;
     width: 100%;
     height: 14px;
     line-height: 14px;
     text-align: center;
     font-family: PingFangSC-Light;
     font-size: 13px;
     color: #999999;
}
 .mayFriends .friendsInfo .item .desc{
     margin: 0 auto;
     width: 115px;
     height: 30px;
     line-height: 15px;
     font-family: PingFangSC-Light;
     font-size: 11px;
     color: #999999;
     white-space:normal;
     overflow:hidden;
     display:-webkit-box;
     -webkit-line-clamp:2;
     -webkit-box-orient:vertical 
}
 .mayFriends .friendsInfo .item .btn{
     margin: 15px auto 0 auto;
     width: 110px;
     height: 32px;
     border: 1px solid #333333;
     line-height: 32px;
     text-align: center;
     font-family: PingFangSC-Regular;
     font-size: 13px;
     color: #333333;
}
 .searchCont{
     margin: 9px auto 0 auto;
     width: 90%;
     background: #FFFFFF;
}
 .searchCont .searchNothing{
     margin-top: 19px;
     width:100%;
     text-align: center;
     font-family:PingFangSC-Regular;
     font-size:13px;
     color:#999999;
}
 .searchCont .searchItem{
     margin: 12px auto 0 auto;
     width: 630;
     height: 67px;
     box-sizing: border-box;
     border-bottom: 1px solid #EAEAEA;
}
 .searchCont .searchItem .left{
     float: left;
     width: 230px;
     height: 100%;
}
 .searchCont .searchItem .left .avatar{
     margin: 16px 13px 0 0;
     float: left;
     width: 38px;
     height: 384px;
}
 .searchCont .searchItem .left .avatar wx-image{
     width: 38px;
     height: 38px;
     border-radius: 50%;
}
 .searchCont .searchItem .left .msg{
     float: left;
     width: 177px;
}
 .searchCont .searchItem .left .msg .name{
     margin-top: 14px;
     font-family: PingFangSC-Regular;
     font-size: 15px;
     color: #333333;
     width: 100%;
     height: 24px;
     line-height: 24px;
     overflow:hidden;
     display:-webkit-box;
     -webkit-line-clamp:1;
     -webkit-box-orient:vertical 
}
 .searchCont .searchItem .left .msg .date{
     margin: 1px 0 4px 0;
     font-family: PingFangSC-Light;
     font-size: 13px;
     color: #999999;
}
 .searchCont .searchItem .right{
     margin-top:14px;
     float: right;
}
 .searchCont .searchItem .day{
     text-align: right;
     font-family: PingFangSC-Regular;
     font-size: 11px;
     color: #FF3939;
}
 .searchCont .searchItem .nextBirthday{
     text-align: right;
     font-family: PingFangSC-Light;
     font-size: 11px;
     color: #999999;
}
