.container{ padding:115px 28px 53px; color: #999; font-size: 13px; background-image: url('http://ovhvevt35.bkt.clouddn.com/day/bg-group.jpg'); background-repeat: no-repeat; background-color: #f8f8f8; background-size: 100%; }
.cy{ color: #CC9E3D; }
.group-info{ display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; padding: 0 37px 20px; width:80%;}
.group-info image{
  width:220rpx; 
  cursor: pointer;
}
.group-info:nth-child(1){ padding: 0 0 20px; }
.group-info .group-info-left .group-name{ font-size: 23px; color: #333; width:264px; padding-bottom: 9px; }
.group-info .group-info-left .group-num{ font-size: 11px; }
.other-btn{ width: 32px; height: 32px; -webkit-box-sizing: border-box; box-sizing: border-box; line-height: 32px; font-size: 12px; border-radius: 50%; text-align: center;}
.other-btn wx-image{ width: 32px; height:32px; }
.order{ width:182px; height:26px; line-height: 26px; margin: 0 auto 17px; text-align: center; border-radius: 13px; font-size: 11px; background: #FAF6EB; padding:0 4px 0 10px; box-sizing: border-box; font-family: PingFangSC-Light; }
.order wx-text{ margin-left: 11px; }
.group-member-birth{ background: url('http://ovhvevt35.bkt.clouddn.com/day/birthcard.png') no-repeat; -webkit-background-size: 100%; background-size: 100%; height: 95px; border: 1px solid #EDDDBD; display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; box-sizing: border-box; margin-bottom: 9px; }
.group-member-birth .birth-info-spe{ padding:28px 14px 0 0; text-align: right; }
.group-member-birth .birth-info-spe .age >wx-text{ font-size: 19px; }
.group-member-birth .birth-info-spe .age{ font-size: 11px; line-height: 19px; }
.group-member-birth .birth-info-spe .age-nuknow{ font-size: 11px; font-family: PingFangSC-Thin; line-height: 13px; }
.birth-info-spe .today-birth{ padding-bottom: 7px; font-size: 11px; }
.group-member-birth .birth-member-info{ display: -webkit-flex; display: flex; padding: 0 0 0 19px; }
.group-member-birth .birth-member-info .detail-box{ padding-top: 28px; overflow:hidden; }
.group-member-birth .birth-member-info .avatar{ width: 38px; background-image:url('http://ovhvevt35.bkt.clouddn.com/day/ww-dtsr-xk.png'); background-size:100%; margin-right: 7px; height:54px; margin-top: 20px; }
.group-member-birth .birth-member-info .mate-name{ font-size: 15px; color: #333; padding-bottom: 9px; overflow:hidden; text-overflow:ellipsis; }
.group-owner{ display:inline-block; width:26px; height:15px; border:1px solid #CC9E3D; border-radius:7px; font-size:9px; line-height:15px; box-sizing:border-box; text-align:center; vertical-align:middle; margin-right: 2px; }
.self{ font-family: PingFangSC-Light; display:inline-block; width:15px; height:15px; border-radius:7px; font-size:9px; line-height:15px; text-align:center; vertical-align:middle; color: #fff; background: #FF3939; }
.group-member-birth .birth-member-info .mate-birth{ font-size: 13px; color: #999; }
.group-member-birth .birth-member-info wx-image{ width:37px; height:37px; border-radius:100%; margin-top:8px; margin-left:1px; }
.normal-birth{ padding:18px 19px; box-sizing: border-box; display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; border-bottom: 1px solid #E9E9E9; background: #fff; }
.normal-birth .birth-member-info{ padding: 0; }
.normal-birth .birth-member-info{ display: -webkit-flex; display: flex; overflow:hidden; width:196px; }
.normal-birth .birth-member-info .detail-box{ width:150px; }
.normal-birth .birth-member-info .avatar{ width: 38px; margin-right: 7px; }
.normal-birth .birth-member-info .mate-name{ font-size: 15px; color: #333; padding-bottom: 9px; overflow:hidden; text-overflow:ellipsis; }
.normal-birth .birth-member-info .mate-birth{ font-size: 13px; color: #999; }
.normal-birth .birth-member-info wx-image{ width:38px; height:38px; border-radius:100%; }
.join-btn{ margin:38px auto 19px; border:1px solid #FF3939; color: #FF3939; width: 170px; height: 38px; line-height: 38px; -webkit-box-sizing: border-box; box-sizing: border-box; text-align: center; border-radius: 1px; font-size:15px; }
.desc{ color: #999; font-size: 11px; text-align: center; }
.game-btn{ width: 100%; height: 96px; margin-bottom: 9px; padding: 27px 0 27px 76px; box-sizing: border-box; font-family: PingFangSC-Light; }
.game-btn .line1{ font-size: 15px; line-height: 15px; color: #333; padding-bottom: 9px; }
.game-btn .line2{ font-size: 13px; color: #999; }
.game-name-btn{ background: url('http://ovhvevt35.bkt.clouddn.com/day/name-entry.png') no-repeat; background-size:100%; }
.game-birth-btn{ background: url('http://ovhvevt35.bkt.clouddn.com/day/group-entry.png') no-repeat; background-size:100%; }
.game-life-btn{ background: url('http://ovhvevt35.bkt.clouddn.com/day/life-entry.png') no-repeat; background-size: 100%; }
.game-constellation-btn{ background: url('http://ovhvevt35.bkt.clouddn.com/day/characterEntry.png') no-repeat; background-size: 100%; }
.toggle-box{ position: absolute; top:172px; right:16px; background: #fff; box-shadow:1px 5px 4px rgba(0,0,0,0.1); }
.fix-btn{ width: 136px; padding: 14px 0; margin:0 9px; border-bottom: 1px solid #e9e9e9; color: #333; text-align: center; font-size: 15px; box-sizing: border-box; }
