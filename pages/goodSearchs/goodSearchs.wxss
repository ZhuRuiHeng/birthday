

.header-nav { margin: 9px auto 19px auto; background: #fff; -webkit-align-items: center; align-items: center; padding: 9px 19px 0 19px; }
.search-container { position: relative; height: 32px; color: #FFF; border-radius:3px; z-index: 100; overflow: hidden; }
.search { position: absolute; z-index: 110; right: 16px; top: 12px; font-size: 15px; color: #333333; line-height: 29px; padding: 0 9px; }
.search-container .input { background: #F9F9F9; line-height: 32px; color: #999999; padding: 0 28px; font-size: 13px; height: 100%; border-radius: 3px; padding-right: 14px; }
.search-container wx-icon { position: absolute; z-index: 10; left: 8px; top: 8px; }
.top-bar { background: #fff; font-size: 13px; height: 42px; width: 100%; line-height: 39px; white-space: nowrap; }
.top-bar-item { -webkit-flex: 1; flex: 1; display:inline-block; width: 72px; color: #333; text-align: center; font-size: 13px; }
.active{ color: #f00; border-bottom: 2px solid #f00; width: 57px; margin: 0 auto; }
.list-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; }
.item-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:row nowrap; flex-flow:row nowrap; border-bottom:1px solid #eee; margin-top:11px; padding: 1px 19px 11px 19px; }
.right-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; -webkit-flex:1; flex:1; }
.right-top-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:row nowrap; flex-flow:row nowrap; }
.img { width:48px; height:48px; }
.avatar { width:57px; height:57px; border-radius:50%; background-repeat:no-repeat; background-size:contain; margin-right:13px; }
.name { width:48px; height:15px; font-size:15px; color:#333; -webkit-flex:1; flex:1; }
.num { height:13px; font-size:13px; color:#333; margin-right:9px; text-align:center; lint-height:13px; }
.star { width:13px; height:12px; margin-top:2px; }
.birth { color:#999; font-size:13px; line-height:17px; margin-top:5px; }
.desc { color:#999; font-size:13px; line-height:17px; margin-top:4px; margin-bottom:4px; }
.empty-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; -webkit-align-items:center; align-items:center; }
.tip-follow { width:168px; height:124px; background:grey; }
.tip-label { color:#333; width:168px; text-align:center; margin-top:19px; }
.go-follow { color:#fff; background:#FF3939; width:146px; height:38px; line-height:38px; text-align:center; margin-top:38px; }
