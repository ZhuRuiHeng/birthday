.container{ margin-left:28px; margin-right:28px; margin-top:9px; }
.list-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:column nowrap; flex-flow:column nowrap;width:100% }
.item-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:row nowrap; flex-flow:row nowrap; border-bottom:1px solid #eee; padding-top:11px; padding-bottom:11px; }
.right-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; -webkit-flex:1; flex:1; }
.right-top-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:row nowrap; flex-flow:row nowrap; }
.avatar { width:57px; height:57px; border-radius:50%; background-repeat:no-repeat; background-size:contain; margin-right:13px; }
.name { width:38px; height:15px; font-size:15px; color:#333; -webkit-flex:1; flex:1; }
.num { height:13px; font-size:13px; color:#333; margin-right:9px; text-align:center; lint-height:13px; }
.star { width:13px; height:12px; margin-top:2px; }
.birth { color:#999; font-size:13px; line-height:17px; margin-top:5px; }
.desc { color:#999; font-size:13px; line-height:17px; margin-top:4px; margin-bottom:4px; }
.follow { margin-left:auto; margin-right:auto; color:#FF3939; border:1px #FF3939 solid; margin-top:48px; line-height:38px; padding-left:19px; padding-right:19px; font-size:15px; text-align:center; width:96px; margin-bottom:38px; }
.empty-layout { display:-webkit-flex; display:flex; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; -webkit-align-items:center; align-items:center; margin-top:48px; }
.tip-follow { width:211px; height:153px; background:grey; }
.tip-label { color:#333; width:176px; text-align:center; font-size:15px; line-height:19px; }
.go-follow { color:#fff; background:#FF3939; width:146px; height:38px; line-height:38px; text-align:center; font-size:15px; margin-top:19px; }
