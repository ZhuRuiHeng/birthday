.container { margin:1px 19px; }
.info-layout { display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; margin-top:19px; }
.avatar { width:38px; height:38px; border-radius:50%; background-repeat:no-repeat; background-size:contain; margin-right:13px; }
.name { font-size:15px; color:#333; margin-bottom:5px; }
.birth{ font-size:13px; color:#999; }
.main-layout { display:-webkit-flex; display:flex; -webkit-flex:1; flex:1; -webkit-align-items:center; align-items:center; -webkit-flex-flow: row nowrap; flex-flow: row nowrap; height:67px; border-bottom:1px solid #eaeaea; }
.item-layout { display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center; -webkit-flex-flow: row nowrap; flex-flow: row nowrap; }
.choose { width:19px; height:19px; margin-right:19px; }
.name-layout { display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center; -webkit-flex-flow:column nowrap; flex-flow:column nowrap; margin-top:19px; margin-bottom:19px; }
.name-label { color:#333; font-size:15px; }
.label { font-size:11px; color:#999; margin-top:4px; }
.btn-layout { position: fixed;bottom: 0;left: 0;background: #fff;
  height:100rpx; display:-webkit-flex; display:flex; -webkit-flex-flow:row nowrap; flex-flow:row nowrap; -webkit-align-items:center; align-items:center; -webkit-justify-content: space-around; 
  justify-content: space-around; width:90%;margin-left:5%;}
.cancel { -webkit-flex:1; flex:1; height:38px; color:#333; line-height:38px; text-align:center; margin-right:4px; font-size:13px; border:1px solid #999; box-sizing: border-box}
.confirm { -webkit-flex:1; flex:1; height:38px; color:#fff; background-color:#ff3939; line-height:38px; text-align:center; font-size:13px; }
.content{
  margin-bottom: 160rpx;
  width: 100%;
}
