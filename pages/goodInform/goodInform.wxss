.container{ font-family: PingFangSC-Medium; font-size: 13px; text-align: center; color: #333; }
.header-image{ width:100%; height:192px; }
.rank-box{ margin:-34px 19px 0; height: 67px; background: #FAF6EB; border: 1px solid #EDDDBD; display: -webkit-flex; display: flex; -webkit-justify-content: space-between; justify-content: space-between; padding: 14px 19px 0 14px; box-sizing: border-box; }
.rank-num{ margin-top: -24px; width: 67px; height: 53px; background:url('https://static.shengri.cn/uploads/image/app/wxapp/rank_bg_normal.png'); color: #fff; padding:6px 9px 8px; line-height: 19px; box-sizing: border-box; background-size: 100%; }
.no1{ background: url('https://static.shengri.cn/uploads/image/app/wxapp/rank_bg_1.png'); background-size: 100%; }
.rank-desc{ font-family: PingFangSC-Semlbold; color: #CC9E3D; text-align:right; }
.star-name{ font-family: PingFangSC-Regular; font-size: 23px; line-height: 23px; padding-top: 28px; padding-bottom: 9px; }
.star-birth{ font-family: PingFangSC-Light; font-size: 11px; }
.like{ margin:0 auto; height:23px; line-height: 23px; color: #999999; font-size: 11px; display: -webkit-flex; display: flex; -webkit-justify-content: center; justify-content: center; }
.like wx-image{ width: 23px; height:23px; border-radius: 100%; display: inline-block; margin-right: 4px; }
.self-like-num{ color: #999; font-size: 11px; border-bottom: 1px solid #999; position: relative; padding-right:19px; }
.self-like-num::after{ border:5px solid #999; border-right:5px solid transparent; border-top:2px solid transparent; border-bottom:2px solid transparent; content:""; width:0; height:0; position:absolute; top:50%; -webkit-transform:translate3d(0,-50%,0); transform:translate3d(0,-50%,0); right:-2px; }
.today { background:#ffecec; border:1px solid #FF3939; color:#ff0000; margin-left:9px; padding:1px 4px; border-radius: 1px; font-size:11px; font-family: BodoniSvtyTwoITCTT-Book; }
.gender_pic_male{ width:8px; height:8px; margin-top:23px; }
.gender_pic_female{ width:6px; height:9px; margin-top:20px; }
.introduction { color: #999; font-size:11px; margin: 9px 28px; font-family: PingFangSC-Light; }
.total-like{ font-size: 28px; color: #FF3939; margin: 28px 0; position: relative; }
.total-like wx-image{ width: 13px; height: 13px; text-align: center; display:inline-block; vertical-align: middle; }
.like-btn{ background: #FF3939; border-radius: 100%; box-shadow: rgba(255,57,57,0.6) 0 3px 7px 0; width:76px; height:76px; margin: 0 auto 48px; color: #fff; padding-top: 20px; box-sizing: border-box; font-size: 11px; position: relative; }
.like-btn wx-image{ background-color: #FF3939; width: 21px; height: 21px; margin: 0 auto; margin-bottom: 9px; }
.btn-cover{ position:absolute; top:0; background:rgba(0,0,0,0.15); height:76px; width:76px; border-radius:100%; }
.anli{ position: relative; width:100%;height: 240px}
.anli wx-image{ width:100%; height:240px; }
.anli button{
  position:absolute;
  z-index:999;
  left:0;
  padding:0;
  border-radius:0;
}
.anli wx-button{ position: absolute; width:100%; height:240px;padding: 0;border-radius:0;}
.tab-page-box{ display: -webkit-flex; display: flex; -webkit-justify-content: space-around; justify-content: space-around; height: 48px; margin-top: 48px; }
.page{ width: 98px; }
.line1{ font-size: 15px; height: 27px; line-height: 24px; margin-bottom: 4px; -webkit-background-size: 100% 100%; background-size: 100% 100%; color: #333; }
.active .line1{ color: #fff; background: #ff4444;}
.active .line2{ color: #FF3939; }
.line2{ font-family: DINCondensed-Bold; font-size: 15px; color: #999999; }
.tips{ color: #FF3939; font-size: 11px; padding-bottom: 43px; }
.rank{ margin:0 19px; display: -webkit-flex; display: flex; height: 58px; margin-bottom: 14px; border-bottom: 1px solid #eaeaea; }
.last { padding-bottom:19px; }
.rank wx-image,.rank wx-view{ display: inline-block; }
.rank wx-view{ padding-top: 16px; }
.num{
  margin:4px 13px 0 0;
  width:38px;
  height:38px;
}
.name{ width:76px; margin-right: 4px; font-family: PingFangSC-Regular; font-size: 15px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; }
.gender{ font-size: 15px; color: #FF3939; width: 24px; font-family: PingFangSC-Light; }
.city{ font-family: PingFangSC-Light; color: #999; font-size: 13px; width: 82px; padding-top: 17px; }
.like-num{ font-size: 15px; text-align: right; }
.follow{ position: absolute; top:19px; left:294px; color: #CC9E3D; line-height: 15px; }
.countdown{ width:48px; height:48px; background: #FF3939; color: #fff; position: absolute; top:15px; left:0; font-size: 9px; padding-top: 2px; box-sizing: border-box; }
.tip-text{ font-family: PingFangSC-Light; font-size: 11px; }
.empty{ margin-top:38px; margin-bottom: 24px; color: #999; }
.empty wx-image{ width: 229px; height: 44px; margin: 0 auto 28px; }
.empty wx-button{ width: 172px; margin: 0 auto; height: 38px; border:1px solid #FF3939; line-height: 38px; font-size: 15px; color: #FF3939; margin-top: 28px; margin-bottom: 19px; border-radius: 0; }
.shareBtn{ font-family: PingFangSC-Light; margin-top: 28px; margin-bottom: 9px; position: relative; }
.shareBtn wx-button{ position: absolute; width:100%; height:100%; opacity:0; }
.groupName{ color: #333333; font-size: 19px; margin-bottom: 9px; }
.group-info{ width:273px; margin: 0 auto; background: #F8F8F8; color: #999999; font-style: 11px; height: 21px; line-height: 21px; border-radius: 10px; position: relative; }
.group-info::after{ border:5px solid #999; border-right:5px solid transparent; border-top:2px solid transparent; border-bottom:2px solid transparent; content:""; width:0; height:0; position:absolute; top:8px; right:1px; }
@-webkit-keyframes baojiMove { 0% { top: 28px; opacity: 1; }
100% { top: 0; opacity: 0; }
}@keyframes baojiMove { 0% { top: 28px; opacity: 1; }
100% { top: 0; opacity: 0; }
}.baoji-wrap { position: absolute; z-index: 99; width: 86px; height: 43px; top: 19px; left: 177px; }
.baoji-tip { position: absolute; top: 28px; width: 100%; height: 14px; line-height: 14px; text-align: center; color: #ff3939; font-size: 13px; -webkit-animation: baojiMove 1s linear forwards; animation: baojiMove 1s linear forwards; }
.content{
  width: 100%;
}
.group .title,.items .notice{
  text-align: center;
  font-size: 26rpx;
  color:#999;
}
.group .shareQun{
  border:1px solid #ff3939;
  background: #eaeaea;
  height: 80rpx;
  line-height: 80rpx;
  font-size:30rpx;
  width: 300rpx;
  margin: 30rpx auto;
  border-radius:0;
  color: #ff3939;
}
