.group{
     padding:0 28px 28px;
     color: #333;
     font-size: 15px;
     font-family: PingFangSC-Regular;
     box-sizing: border-box;
}
 .group .empty-box{
   text-align: center;
}
 .group .empty-box wx-image{
     width: 133px;
     height: 160px;
     margin: 81px auto 38px;
}
 .group .empty-box .line1{
     font-size: 13px;
     color: #333;
     line-height: 19px;
     text-align: center;
}
 .group .empty-box .line2{
     font-size: 13px;
     color: #999;
     text-align: center;
}
 .group .empty-box .line2 .question-group{
     font-size: 13px;
     width: 15px;
     height: 15px;
     color: #999;
     text-align: center;
     display: inline-block;
     border: 1px solid #999;
     border-radius: 100%;
     line-height: 15px;
     margin-left: 3px;
}
 .group .empty-box .creat-btn{
     width: 146px;
     height: 38px;
     background: #FF3939;
     margin: 28px auto 63px;
     text-align: center;
     color: #fff;
     line-height: 38px;
     border-radius: 1px;
     cursor: pointer;
}
 .group-member-box{
     display: -webkit-flex;
     display: flex;
     -webkit-justify-content: space-between;
     justify-content: space-between;
     height: 43px;
     margin-bottom: 10px;
}
 .group .group-item{
     margin-top: 28px;
     padding-bottom:28px;
      border-bottom:1px solid #e8e8e8; 
}
 .group-item .group-detail{
     display: -webkit-flex;
     display: flex;
     -webkit-justify-content: space-between;
     justify-content: space-between;
     padding-bottom: 14px;
}
 .group-member-box .group-num{
     padding-top:31px;
     font-size: 11px;
     line-height: 11px;
     color: #999;
}
 .group-item .group-mate{
     display: -webkit-flex;
     display: flex;
     -webkit-justify-content: flex-start;
     justify-content: flex-start;
}
 .group-mate wx-image{
     height: 38px;
     width: 38px;
     margin-right: 9px;
     border-radius: 100%;
}
 .birth-info{
     display: -webkit-flex;
     display: flex;
     background-color: #f9f9f9;
     border-radius: 1px;
     -webkit-flex-direction: column;
     flex-direction: column;
}
 .birth-info .yihang{
     display: -webkit-flex;
     display: flex;
     -webkit-justify-content: space-between;
     justify-content: space-between;
     -webkit-align-items: baseline;
     align-items: baseline;
}
 .birth-info .icon-1{
     margin:8px;
     width:16px;
}
 .birth-info .icon-1 wx-image{
     width:16px;
     height: 14px;
}
 .birth-info .icon-2{
     margin:7px;
}
 .birth-info .icon-2 wx-image{
     width:16px;
     height: 16px;
}
 .cr{
     color: #FF3939;
}
 .cg{
     color:#999999;
}
 .birth-info .bg{
     background: rgba(153,153,153,0.1);
}
 .birth-info{
     font-size: 13px;
     line-height: 31px;
}
 .birth-day{
     margin-right: 4px;
     font-size: 13px;
}
 .birth-desc{
     margin-right: 4px;
     font-size: 13px;
     -webkit-flex: 1;
     -ms-flex: 1;
     flex: 1;
}
 .birth-date{
     width:34px;
     height:16px;
     border-radius:8px;
     margin:7px 0;
     line-height:16px;
     font-size:11px;
     text-align:center;
     background:rgba(255,57,57,0.1);
     margin-right: 4px;
}
 .add-group{
     padding-top: 53px;
     text-align: center;
}
 .add-group .line{
     font-size: 11px;
     color: #999;
     padding-bottom: 12px;
}
 .add-group .add-group-btn{
     font-size: 15px;
     color: #FF3939;
}
